nt />\n    <h3>INCIO ESTUDIANTE</h3>\n    <button>\n      <router-link :to=\"{ name: 'NewQuestion' }\">Hacer pregunta</router-link>\n    </button>\n    <!--COMPONENTE DE BUSQUEDA-->\n    <searchcomponent v-on:queryParams=\"collectParams\" :languages=\"languages\" />\n    <!--ACTIVIDAD RECIENTE-->\n    <div v-show=\"showQuestion\">\n      <h3>ACTIVIDAD RECIENTE</h3>\n      <ShowQuestionsAnswer\n        :questions=\"UserQuestion\"\n        :answer=\"UserAnswers\"\n        :count=\"count\"\n      />\n      <div v-show=\"noQuestions\">\n        <p>Aww... aun no has preguntado nada aún</p>\n        <p>¿Quieres hacer una?</p>\n        <router-link :to=\"{ name: 'NewQuestion' }\">Hacer pregunta</router-link>\n      </div>\n    </div>\n    <p v-show=\"showError\">{{ errorMessage }}</p>\n    <!--RESULTADO DE BUSQUEDA-->\n    <getquestions v-show=\"allQuestions\" :questions=\"questions\" />\n  </div>\n</template>\n\n<script>\n//IMPORTAR COMPONENTES\nimport getquestions from \"@/components/GetQuestions.vue\";\nimport searchcomponent from \"@/components/SearchComponent.vue\";\nimport menustudent from \"@/components/MenuStudent\";\nimport ShowQuestionsAnswer from \"@/components/GetQuestionAnswer.vue\";\n\n//IMPORTAR UTILIDADES\nimport { format, formatDistance } from \"date-fns\";\nimport es from \"date-fns/locale/es\";\nimport axios from \"axios\";\nimport { getAuthToken, getRoleToken, config, ENDPOINT } from \"../utils/helpers\";\n\nexport default {\n  name: \"HomeExpert\",\n  components: {\n    menustudent,\n    getquestions,\n    searchcomponent,\n    ShowQuestionsAnswer,\n  },\n  data() {\n    return {\n      //variables de gestion de errores\n      showError: false,\n      errorMessage: \"\",\n\n      UserQuestion: [],\n      UserAnswers: [],\n      token: getAuthToken(),\n      hideQuestion: false,\n      showQuestion: true,\n      noQuestions: false,\n      count: \"\",\n\n      //Variables de componente\n      title: \"\",\n      content: \"\",\n      idQuestion: \"\",\n\n      questions: [],\n      languages: [],\n      allQuestions: false,\n    };\n  },\n  methods: {\n    //DESDE LA BBDD\n    async getQuestionsAnwer() {\n      try {\n        const response = await axios.get(ENDPOINT + \"/my-questions/\", config);\n\n        let resultNumber = response.data.resultAnswers;\n        if (resultNumber.length === 0) {\n          this.noQuestions = true;\n        } else {\n          this.UserQuestion = response.data.resultAnswers;\n        }\n      } catch (error) {\n        this.showError = true;\n        console.log(error);\n        this.errorMessage = error.response.data.message;\n      }\n    },\n    //RECIBO LOS PARAMETROS DE BUSQUEDA DESDE MI COMPONENTE\n    async collectParams(componentParams) {\n      this.successSearch = true;\n      this.showError = false;\n      this.queryParams = componentParams;\n      this.showQuestion = false;\n      await this.getQuestions();\n    },\n    //TRAER LOS LENGUAGES DE LA BBDD PARA EL SELECTOR DE LA BUSQUEDA AVANZADA\n    async getLanguages() {\n      try {\n        const response = await axios.get(ENDPOINT + \"/languages\");\n        this.languages = response.data.data;\n      } catch (error) {\n        this.showError = true;\n        this.errorMessage = error.response.data.message;\n      }\n    },\n    //TRAIGO TODAS LAS PREGUNTAS DE LA BBDD\n    async getQuestions() {\n      try {\n        const response = await axios.get(ENDPOINT + \"/questions\", {\n          params: this.queryParams,\n        });\n\n        this.questions = response.data.data;\n        this.allQuestions = true;\n      } catch (error) {\n        this.showError = true;\n        this.errorMessage = error.response.data.message;\n\n        if (\n          this.errorMessage === \"No hay preguntas que coincidan con tu busqueda\"\n        ) {\n          this.successSearch = false;\n        }\n      }\n    },\n  },\n  created() {\n    this.getLanguages();\n    this.getQuestionsAnwer();\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views"}]}