n          config\n        );\n        this.username = response.data.data.username;\n        this.avatar = process.env.VUE_APP_STATIC + response.data.data.avatar;\n      } catch (error) {\n        this.showError = true;\n        this.errorMessage = error.response;\n      }\n    },\n  },\n  async created() {\n    this.token = getAuthToken();\n    await this.getUserProfile(this.token);\n  },\n};\n</script>\n\n<style scoped>\n#topBar {\n  background-color: var(--darkColor);\n  padding: 1.5rem 3rem;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: space-between;\n  position: sticky;\n  top: 0;\n}\n\n.lateralmenu {\n  background-color: var(--ligthColor);\n}\n\n.metadataUser {\n  display: none;\n}\n.footer {\n  position: fixed;\n  bottom: 0;\n  width: 100%;\n}\n\n@media only screen and (min-width: 600px) {\n  #topBar {\n    padding: 1rem 3rem;\n    background-color: var(--darkColor);\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: space-between;\n  }\n  #topBar .metadataUser {\n    display: flex;\n    direction: flex;\n    flex-direction: row;\n    justify-content: baseline;\n    align-items: center;\n  }\n}\n\n@media only screen and (min-width: 1200px) {\n  .logoMenu {\n    margin-right: 50px;\n  }\n\n  .main {\n    padding-left: 296px;\n    /* background-color: darkviolet; */\n  }\n}\n</style>"],"sourceRoot":"src/layouts"}]}