AjBA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAmBA,KAAA,CAAA,IAAA,CACA,gCADA,EAEA,IAFA,CAnBA;;AAAA;AAmBA,gBAAA,QAnBA;AAwBA,gBAAA,KAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA,CAAA,aAAA;AAEA,gBAAA,aAAA,CACA,SADA,EAEA,kBAFA,8BAGA,IAAA,CAAA,KAHA,yFAAA;AA1BA;AAAA;;AAAA;AAAA;AAAA;AAiCA,gBAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,gBAAA,KAAA,CAAA,YAAA,GAAA,YAAA,QAAA,CAAA,IAAA,CAAA,OAAA;;AAlCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqCA,KA5CA;AA6CA,IAAA,YA7CA,0BA6CA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,eAAA,GAAA,EAAA;AACA;AAlDA;AAlBA,CAAA","sourcesContent":["<template>\n  <div>\n    <vue-headful title=\"Login | HackaClients\" description=\"Pagina de login\" />\n    <div>\n      <!--DIV PARA COLOCAR LA IMAGEN DE TUTORSHIPS-->\n    </div>\n    <div>\n      <div>\n        <p class=\"accesibilityTxtalert\" v-show=\"showError\">{{ errorMessage }}</p>\n        <h3>EMAIL:</h3>\n        <input type=\"email\" v-model=\"email\" placeholder=\"miemail@tutorships.com\" />\n        <h3>NOMBRE DE USUARIO:</h3>\n        <input @click=\"showText()\" type=\"text\" v-model=\"username\" placeholder=\"youngcoderpadawan\" />\n        <p\n          class=\"accesibilityTxt\"\n          v-show=\"coment\"\n        >Puedes usar letras, númenos y signos de puntuación.</p>\n        <p></p>\n        <h3>CONTRASEÑA:</h3>\n        <input @click=\"showTextPass()\" type=\"password\" v-model=\"password\" placeholder=\"********\" />\n        <p class=\"accesibilityTxt\" v-show=\"comentPass\">\n          Usa 8 o más caracteres con una combinación de letras, números y\n          símbolos.\n        </p>\n        <h3>REPITE LA CONTRASEÑA:</h3>\n        <input type=\"password\" v-model=\"confirmpassword\" placeholder=\"*******\" />\n        <p class=\"accesibilityTxtalert\" v-show=\"alert\">Tu contraseña debe coincidir.</p>\n        <button class=\"cancelButton\" @click=\"cancelAction()\">Cancelar</button>\n        <button @click=\"registerUser()\">¡Registrarme!</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\nimport { alertFunction } from \"../utils/helpers\";\n\nexport default {\n  name: \"Register\",\n  data() {\n    return {\n      email: \"\",\n      username: \"\",\n      password: \"\",\n      confirmpassword: \"\",\n      coment: false,\n      comentPass: false,\n      alert: false,\n      validationUrl: \"\",\n\n      //variables de gestion de errores\n      showError: false,\n      errorMessage: \"\",\n    };\n  },\n  methods: {\n    showText() {\n      this.coment = true;\n    },\n    showTextPass() {\n      this.comentPass = true;\n    },\n    async registerUser() {\n      let data = {\n        username: this.username,\n        email: this.email,\n        password: this.password,\n      };\n\n      /* if (\n        this.email === \"\" ||\n        this.username === \"\" ||\n        this.password === \"\" ||\n        this.confirmpassword === \"\"\n      ) {\n        this.showError = true;\n        this.errorMessage = \"¡Oh! :( Debes llenar todos los campos!\";\n      } */\n\n      if (this.password === this.confirmpassword) {\n        try {\n          const response = await axios.post(\n            \"http://localhost:3000/new-user\",\n            data\n          );\n\n          this.validationUrl = response.data.activationUrl;\n\n          alertFunction(\n            \"success\",\n            \"Registro Exitoso\",\n            `Confirma tu email ${data.email} para activar tu cuenta en Tutorships.\n        Recuerda revisar un carpeta de SPAM`\n          );\n        } catch (error) {\n          this.showError = true;\n          this.errorMessage = error.response.data.message;\n        }\n      }\n    },\n    cancelAction() {\n      this.email = \"\";\n      this.username = \"\";\n      this.password = \"\";\n      this.confirmpassword = \"\";\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views"}]}