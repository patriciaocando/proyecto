ror(error);\n        });\n    },\n    //TRAER EL LENGUAGE ESCOGIDO DEL COMPONENTE/SELECT LENGUAJES\n    getComponentLanguage(languageId) {\n      this.language = languageId;\n    },\n    //PUBLICAR LA RESPUESTA\n    postQuestion() {\n      let self = this;\n      let data = {\n        title: self.title,\n        content: self.content,\n        language: self.language,\n      };\n\n      let config = {\n        headers: {\n          Authorization: self.token,\n        },\n      };\n      console.log(data);\n      axios\n        .post(\"http://localhost:3000/new-question\", data, config)\n        .then(function (response) {\n          console.log(response);\n        })\n        .catch(function (error) {\n          console.error(error);\n        });\n      this.alertFunction();\n      //ALERT DE CREACIOND E PREGUNTA\n      //VACIAR LOS CAMPOS\n      //RELOAD PAGE\n    },\n  },\n  created() {\n    this.getLanguages(\n      \"success\",\n      \"Pregunta publicada\",\n      \"Se ha publicado tu pregunta exitosamente.\"\n    );\n  },\n};\n</script>\n\n<style scoped>\n</style>"],"sourceRoot":"src/views"}]}