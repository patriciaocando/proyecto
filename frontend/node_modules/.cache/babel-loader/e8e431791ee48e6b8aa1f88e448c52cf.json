is.idUserRequest,\n          { languages: this.languagesRequest },\n          config\n        );\n        alertFunction(\n          \"success\",\n          \"Nuevo Experto\",\n          `Has dado de alta a un nuevo experto en TutorShips`\n        );\n      } catch (error) {\n        this.showError = true;\n\n        this.errorMessage = error.response.data.message;\n      }\n    },\n  },\n  created() {\n    this.getRequestData();\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views"}]}