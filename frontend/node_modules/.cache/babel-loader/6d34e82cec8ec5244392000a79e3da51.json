confirmpassword: \"\",\n      coment: false,\n      comentPass: false,\n      alert: false,\n      validationUrl: \"\",\n    };\n  },\n  methods: {\n    showText() {\n      this.coment = true;\n    },\n    showTextPass() {\n      this.comentPass = true;\n    },\n    //ALERT DE EDITAR\n    updateUserAlert(email) {\n      Swal.fire({\n        icon: \"success\",\n        title: \"Registro Exitoso\",\n        text: `Confirma tu email ${email} para activar tu cuenta en Tutorships.\n        Recuerda revisar un carpeta de SPAM`,\n      });\n    },\n\n    registerUser() {\n      let self = this;\n      if (self.password === self.confirmpassword) {\n        axios\n          .post(\"http://localhost:3000/new-user\", {\n            username: self.username,\n            email: self.email,\n            password: self.password,\n          })\n          .then(function (response) {\n            self.validationUrl = response.data.activationUrl;\n            console.log(response);\n          })\n          .catch(function (error) {\n            console.error(error);\n          });\n        self.updateUserAlert(self.email);\n      } else {\n        self.alert = true;\n      }\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views"}]}