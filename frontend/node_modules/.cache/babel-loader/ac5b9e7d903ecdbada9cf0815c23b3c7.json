</h3>\n        <p>{{ expert.name_language }}</p>\n        <h3>BIO:</h3>\n        <p>{{ expert.profile_bio }}</p>\n        <h3>MIEMBRO DESDE:</h3>\n        <p>{{ expert.creation_date }}</p>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"Experts\",\n\n  data() {\n    return {\n      experts: [],\n    };\n  },\n\n  methods: {\n    //TRAIGO TODAS LAS PREGUNTAS DE LA BBDD\n    getExperts() {\n      let self = this;\n      axios\n        .get(\"http://localhost:3000/experts\")\n        .then(function (response) {\n          self.experts = response.data.data;\n        })\n        .catch(function (error) {\n          console.error(error);\n        });\n    },\n  },\n  created() {\n    this.getExperts();\n  },\n};\n</script>\n\n<style scoped>\n.avatar {\n  width: 48px;\n  height: 48px;\n  border-radius: 24px;\n}\n</style>"],"sourceRoot":"src/views"}]}