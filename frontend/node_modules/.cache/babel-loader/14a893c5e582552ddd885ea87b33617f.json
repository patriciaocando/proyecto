KAAA,CAAA,eAAA,KAAA,EAAA,EAAA;AACA,kBAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,kBAAA,KAAA,CAAA,YAAA,GAAA,wBAAA;AACA;;AAVA,sBAYA,KAAA,CAAA,QAAA,KAAA,KAAA,CAAA,eAZA;AAAA;AAAA;AAAA;;AAaA,gBAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,gBAAA,KAAA,CAAA,YAAA,GAAA,+BAAA;AAdA;AAAA;;AAAA;AAAA;AAAA;AAAA,uBAiBA,KAAA,CAAA,IAAA,CACA,gCADA,EAEA,YAFA,CAjBA;;AAAA;AAiBA,gBAAA,QAjBA;AAsBA,gBAAA,KAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA,CAAA,aAAA;AAtBA;AAAA,uBAwBA,aAAA,CACA,SADA,EAEA,kBAFA,8BAGA,KAAA,CAAA,KAHA,2FAxBA;;AAAA;AA8BA,gBAAA,KAAA,CAAA,YAAA,GAAA,KAAA;AACA,gBAAA,KAAA,CAAA,YAAA,GAAA,IAAA;AA/BA;AAAA;;AAAA;AAAA;AAAA;AAiCA,gBAAA,OAAA,CAAA,GAAA;AACA,gBAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,gBAAA,KAAA,CAAA,YAAA,GAAA,YAAA,QAAA,CAAA,IAAA,CAAA,OAAA;;AAnCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsCA,KA7CA;AA8CA,IAAA,YA9CA,0BA8CA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,eAAA,GAAA,EAAA;AACA;AAnDA;AAtBA,CAAA","sourcesContent":["<template>\n  <div>\n    <vue-headful title=\"Login | HackaClients\" description=\"Pagina de login\" />\n    <div>\n      <!--DIV PARA COLOCAR LA IMAGEN DE TUTORSHIPS-->\n    </div>\n\n    <div>\n      <img src=\"../assets/logoTutorShips.png\" />\n      <!-- FORMULARIO DE REGISTRO -->\n      <div v-show=\"registerShow\">\n        <h3>EMAIL:</h3>\n        <input type=\"email\" v-model=\"email\" placeholder=\"miemail@tutorships.com\" />\n        <h3>NOMBRE DE USUARIO:</h3>\n        <input @click=\"showText()\" type=\"text\" v-model=\"username\" placeholder=\"youngcoderpadawan\" />\n        <p\n          class=\"accesibilityTxt\"\n          v-show=\"coment\"\n        >Puedes usar letras, númenos y signos de puntuación.</p>\n\n        <h3>CONTRASEÑA:</h3>\n        <input @click=\"showTextPass()\" type=\"password\" v-model=\"password\" placeholder=\"********\" />\n        <p class=\"accesibilityTxt\" v-show=\"comentPass\">\n          Usa 8 o más caracteres con una combinación de letras, números y\n          símbolos.\n        </p>\n        <h3>REPITE LA CONTRASEÑA:</h3>\n        <input type=\"password\" v-model=\"confirmpassword\" placeholder=\"*******\" />\n\n        <p class=\"accesibilityTxtalert\" v-show=\"showError\">{{ errorMessage }}</p>\n        <button class=\"cancelButton\" @click=\"cancelAction()\">Cancelar</button>\n        <button @click=\"registerUser()\">¡Registrarme!</button>\n      </div>\n      <!-- DIV REVISA TU EMAL -->\n      <div v-show=\"registerHide\">\n        <h1>¡Revisa tu email!</h1>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\nimport { alertFunction } from \"../utils/helpers\";\n\nexport default {\n  name: \"Register\",\n  data() {\n    return {\n      //variables de componente\n      email: \"\",\n      username: \"\",\n      password: \"\",\n      confirmpassword: \"\",\n\n      //variables de vista\n      registerShow: true,\n      registerHide: false,\n      validationUrl: \"\",\n      coment: false,\n      comentPass: false,\n\n      //variables de gestion de errores\n      showError: false,\n      errorMessage: \"\",\n    };\n  },\n  methods: {\n    showText() {\n      this.coment = true;\n    },\n    showTextPass() {\n      this.comentPass = true;\n    },\n    async registerUser() {\n      let registerData = {\n        username: this.username,\n        email: this.email,\n        password: this.password,\n      };\n\n      if (this.confirmpassword === \"\") {\n        this.showError = true;\n        this.errorMessage = \"Confirma tu contraseña\";\n      }\n\n      if (this.password !== this.confirmpassword) {\n        this.showError = true;\n        this.errorMessage = \"Tus contraseña debe coincidir\";\n      } else {\n        try {\n          const response = await axios.post(\n            \"http://localhost:3000/new-user\",\n            registerData\n          );\n\n          this.validationUrl = response.data.activationUrl;\n\n          await alertFunction(\n            \"success\",\n            \"Registro Exitoso\",\n            `Confirma tu email ${this.email} para activar tu cuenta en Tutorships.\n          Recuerda revisar un carpeta de SPAM`\n          );\n          this.registerShow = false;\n          this.registerHide = true;\n        } catch (error) {\n          console.log(error);\n          this.showError = true;\n          this.errorMessage = error.response.data.message;\n        }\n      }\n    },\n    cancelAction() {\n      this.email = \"\";\n      this.username = \"\";\n      this.password = \"\";\n      this.confirmpassword = \"\";\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views"}]}