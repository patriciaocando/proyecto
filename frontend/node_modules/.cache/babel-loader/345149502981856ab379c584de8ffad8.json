,QAAA,KAAA,SADA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA,uBAGA,KAAA,CAAA,GAAA,CAAA,QAAA,GAAA,mBAAA,EAAA;AACA,kBAAA,OAAA,EAAA;AACA,oBAAA,aAAA,EAAA,YAAA,CAAA,OAAA,CAAA,gBAAA;AADA;AADA,iBAAA,CAHA;;AAAA;AAGA,gBAAA,QAHA;AASA,gBAAA,KAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AATA;AAAA;;AAAA;AAAA;AAAA;AAWA,gBAAA,KAAA,CAAA,SAAA,GAAA,IAAA;AACA,gBAAA,KAAA,CAAA,YAAA,GAAA,YAAA,QAAA,CAAA,IAAA,CAAA,OAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAeA,KArBA;AAsBA;AACA,IAAA,YAvBA,0BAuBA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,MAAA,CAAA,gBAAA,EAFA;;AAAA;AAAA;AAAA,uBAGA,KAAA,CAAA,GAAA,CAAA,QAAA,GAAA,YAAA,CAHA;;AAAA;AAGA,gBAAA,QAHA;AAIA,gBAAA,MAAA,CAAA,aAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AAJA,uDAMA,MAAA,CAAA,aANA;;AAAA;AAMA,sEAAA;AAAA,oBAAA,QAAA;AAAA,4DACA,MAAA,CAAA,aAAA,CAAA,OAAA,EADA;;AAAA;AACA,6EAAA;AAAA,wEAAA,KAAA,oBAAA,eAAA;;AACA,4BAAA,QAAA,CAAA,EAAA,KAAA,eAAA,CAAA,EAAA,EAAA;AACA,0BAAA,MAAA,CAAA,aAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AACA;AALA;AAAA;AAAA;AAAA;AAAA;AAMA;AAZA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAcA,gBAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,YAAA,GAAA,aAAA,QAAA,CAAA,IAAA,CAAA,OAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA,KAxCA;AAyCA;AACA,IAAA,kBA1CA,8BA0CA,UA1CA,EA0CA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CACA,QAAA,GAAA,yBAAA,GAAA,UADA,EAEA;AACA,kBAAA,OAAA,EAAA;AACA,oBAAA,aAAA,EAAA,YAAA,CAAA,OAAA,CAAA,gBAAA;AADA;AADA,iBAFA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAAA;AAAA,uBAUA,MAAA,CAAA,gBAAA,EAVA;;AAAA;AAAA;AAAA,uBAWA,MAAA,CAAA,YAAA,EAXA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAaA,gBAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,YAAA,GAAA,aAAA,QAAA,CAAA,IAAA,CAAA,OAAA;;AAdA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgBA,KA1DA;AA2DA;AACA,IAAA,WA5DA,uBA4DA,UA5DA,EA4DA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA;AACA,gBAAA,IAHA,GAGA;AACA,kBAAA,WAAA,EAAA;AADA,iBAHA;AAAA;AAAA,uBAMA,KAAA,CAAA,GAAA,CACA,QAAA,GAAA,qBADA,EAEA,IAFA,EAGA,MAHA,CANA;;AAAA;AAMA,gBAAA,QANA;AAAA;AAAA,uBAWA,MAAA,CAAA,gBAAA,EAXA;;AAAA;AAAA;AAAA,uBAYA,MAAA,CAAA,YAAA,EAZA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAcA,gBAAA,MAAA,CAAA,SAAA,GAAA,IAAA;AACA,gBAAA,MAAA,CAAA,YAAA;;AAfA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAiBA;AA7EA,GAvBA;AAsGA,EAAA,OAtGA,qBAsGA;AACA,SAAA,YAAA;AACA;AAxGA,CAAA","sourcesContent":["<template>\n  <div class=\"container\">\n    <!--LENGUAGES DE EXPERTO-->\n    <h1>Lenguajes asociados:</h1>\n    <div class=\"languageContainer\" v-for=\"language in userLanguages\" :key=\"language.id\">\n      <img class=\"languageImage\" :src=\"getImageName(language.image)\" />\n      <div class=\"content\">\n        <span class=\"languageTxt\">\n          <h2>{{ language.language }}</h2>\n          <p>{{ language.description }}</p>\n        </span>\n        <span class=\"buttonContainer\">\n          <button class=\"deleteButton\" @click=\"deleteUserLanguage(language.id)\">Borrar lenguaje</button>\n        </span>\n      </div>\n    </div>\n    <p v-show=\"showError\">{{ errorMessage }}</p>\n\n    <!--TODOS LOS LENGUAGES-->\n    <div class=\"allLanguages\">\n      <h1>Todos los lenguajes:</h1>\n      <div class=\"languageContainer\" v-for=\"languages in bbddLanguages\" :key=\"languages.id\">\n        <img class=\"languageImage\" :src=\"getImageName(languages.image)\" />\n        <div class=\"content\">\n          <span class=\"languageTxt\">\n            <h2>{{ languages.language }}</h2>\n            <p>{{ languages.description }}</p>\n          </span>\n          <span class=\"buttonContainer\">\n            <button @click=\"addLanguage(languages.id)\">ASOCIAR A LENGUAJE</button>\n          </span>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n//STORAGE DE LOS DATOS DE USUARIO\nimport userData from \"@/dataStorage/userData\";\nimport api from \"@/api/api\";\n\nimport axios from \"axios\";\nimport Swal from \"sweetalert2\";\nimport {\n  getAuthToken,\n  getIdToken,\n  alertFunction,\n  getRoleToken,\n  config,\n  ENDPOINT,\n} from \"../utils/helpers\";\nexport default {\n  name: \"ExpertLanguages\",\n\n  data() {\n    return {\n      userLanguages: [],\n      bbddLanguages: [],\n      //variables de gestion de errores\n      showError: false,\n      errorMessage: \"\",\n\n      sharedStore: userData.state,\n    };\n  },\n  computed: {\n    token() {\n      return this.sharedStore.token;\n    },\n    userRole() {\n      return this.sharedStore.role;\n    },\n  },\n\n  methods: {\n    getImageName(name) {\n      let url = process.env.VUE_APP_STATIC;\n      return url + name;\n    },\n    //TRAER LOS LENGUAJES ASOCIADOS A UN EXPERTO\n    async getUserLanguages() {\n      if (this.userRole === \"experto\") {\n        try {\n          const response = await axios.get(ENDPOINT + \"/expert/languages\", {\n            headers: {\n              Authorization: localStorage.getItem(\"AUTH_TOKEN_KET\"),\n            },\n          });\n\n          this.userLanguages = response.data.data;\n        } catch (error) {\n          this.showError = true;\n          this.errorMessage = error.response.data.message;\n        }\n      }\n    },\n    //TRAER LOS LENGUAJES DE LA BBDD\n    async getLanguages() {\n      try {\n        await this.getUserLanguages();\n        const response = await axios.get(ENDPOINT + \"/languages\");\n        this.bbddLanguages = response.data.data;\n\n        for (const language of this.userLanguages) {\n          for (const [index, bbddlanguageAux] of this.bbddLanguages.entries()) {\n            if (language.id === bbddlanguageAux.id) {\n              this.bbddLanguages.splice(index, 1);\n            }\n          }\n        }\n      } catch (error) {\n        this.showError = true;\n        this.errorMessage = error.response.data.message;\n      }\n    },\n    //BORRAR LENGUAJE DE UN EXPERTO\n    async deleteUserLanguage(languageId) {\n      try {\n        const response = await axios.delete(\n          ENDPOINT + \"/users/delete-language/\" + languageId,\n          {\n            headers: {\n              Authorization: localStorage.getItem(\"AUTH_TOKEN_KET\"),\n            },\n          }\n        );\n        await this.getUserLanguages();\n        await this.getLanguages();\n      } catch (error) {\n        this.showError = true;\n        this.errorMessage = error.response.data.message;\n      }\n    },\n    //ASOCIAR UN NUEVO LENGUAJE\n    async addLanguage(languageId) {\n      try {\n        //await api.newLanguageExpert(languageId);\n        let data = {\n          newlanguage: languageId,\n        };\n        const response = await axios.put(\n          ENDPOINT + \"/users/add-language\",\n          data,\n          config\n        );\n        await this.getUserLanguages();\n        await this.getLanguages();\n      } catch (error) {\n        this.showError = true;\n        this.errorMessage = error;\n      }\n    },\n  },\n  created() {\n    this.getLanguages();\n  },\n};\n</script>\n\n<style scoped>\n.languageImage {\n  width: 120px;\n  height: 120px;\n  border-radius: 8px;\n}\n\n.allLanguages {\n  margin-top: 3rem;\n  background-color: var(--ligthBlue);\n}\n.allLanguages h1 {\n  padding-top: 2rem;\n  padding-left: 1.5rem;\n}\n.languageContainer {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  margin: 1em 1rem;\n  border-bottom: 1px solid var(--ligthColor);\n  padding: 2rem;\n}\n\n.languageTxt {\n  margin: 0 1rem;\n}\n\n.deleteButton {\n  align-items: flex-end;\n  margin-top: 2rem;\n}\n.allLanguages h3 {\n  margin-top: 2rem;\n}\n\n.content {\n  display: flex;\n  flex-direction: column;\n  align-items: stretch;\n  width: 80%;\n}\n.buttonContainer {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n  align-items: flex-end;\n  justify-content: flex-end;\n}\n</style>\n"],"sourceRoot":"src/views"}]}