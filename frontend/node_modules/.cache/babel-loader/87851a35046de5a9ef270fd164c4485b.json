plate>\n  <div>\n    <vue-headful title=\"Login | HackaClients\" description=\"Pagina de login\" />\n    <div>\n      <!--DIV PARA COLOCAR LA IMAGEN DE TUTORSHIPS-->\n    </div>\n    <div>\n      <div>\n        <h3>EMAIL:</h3>\n        <input type=\"text\" v-model=\"email\" placeholder=\"miemail@tutorships.com\" required />\n        <h3>PASSWORD:</h3>\n        <input type=\"password\" v-model=\"password\" placeholder=\"Password\" required />\n        <p v-show=\"showError\">{{ errorMessage }}</p>\n        <button @click=\"loginUser()\">Login</button>\n        <button class=\"cancelButton\" @click=\"cancelLogin()\">CANCELAR</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\nimport { login } from \"../utils/helpers\";\n\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      showError: false,\n      errorMessage: \"\",\n    };\n  },\n  methods: {\n    cancelLogin() {\n      location.reload();\n    },\n    async loginUser() {\n      if (this.email === \"\" || this.password === \"\") {\n        alert(\"Debes llenar todos los campos\");\n      } else {\n        try {\n          await login(this.email, this.password);\n          this.$router.push(\"/\");\n        } catch (error) {\n          this.showError = true;\n          this.errorMessage = error.response.data.message;\n        }\n      }\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"],"sourceRoot":"src/views"}]}