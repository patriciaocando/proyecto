\nexport default {\n  name: \"UserAnswers\",\n  components: {\n    answercomponent,\n  },\n  data() {\n    return {\n      //Variable de vista\n      bbddAnswers: [],\n      //variables de gestion de errores\n      showError: false,\n      errorMessage: \"\",\n\n      //TOKEN\n      token: getAuthToken(),\n    };\n  },\n  methods: {\n    //TRAER RESPUESTAS HECHAS POR EL USUARUI DESDE LA BBDD\n    async getAnswers() {\n      //CABECERA DE AUTORIZACION\n      let config = {\n        headers: {\n          Authorization: this.token,\n        },\n      };\n\n      try {\n        const response = await axios.get(\n          \"http://localhost:3000/answer\",\n          config\n        );\n        this.bbddAnswers = response.data.data;\n      } catch (error) {\n        this.showError = true;\n        this.errorMessage = error.response.data.message;\n      }\n    },\n    async postEditedAnswer(answerData) {\n      console.log(answerData);\n      try {\n        let config = {\n          headers: {\n            Authorization: this.token,\n          },\n        };\n        let idAnswer = answerData.id;\n        let data = {\n          content: answerData.content,\n        };\n\n        const response = await axios.put(\n          \"http://localhost:3000/edit-answer/\" + idAnswer,\n          data,\n          config\n        );\n        console.log(\"actualizada\");\n      } catch (error) {\n        this.showError = true;\n        this.errorMessage = error.response.data.message;\n      }\n    },\n\n    async deleteQuestion() {\n      //CABECERA DE AUTORIZACION\n      let config = {\n        headers: {\n          Authorization: this.token,\n        },\n      };\n\n      try {\n        const response = await axios.delete(\n          \"http://localhost:3000/delete-question/\" + this.idQuestion,\n          config\n        );\n        this.alertFunction(\n          \"Borrada!\",\n          \"Tu pregunta ha sido borrada.\",\n          \"success\"\n        );\n      } catch (error) {\n        this.showError = true;\n        this.errorMessage = error.response.data.message;\n      }\n    },\n  },\n  created() {\n    this.getAnswers();\n  },\n};\n</script>\n\n<style scoped></style>\n"]}]}