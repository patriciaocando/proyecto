<p>{{ expert.creation_date }}</p>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nexport default {\n  name: \"Experts\",\n\n  data() {\n    return {\n      experts: [],\n      showError: false,\n      errorMessag=\"\",\n    };\n  },\n\n  methods: {\n    getImageName(name) {\n      return process.env.VUE_APP_STATIC + name;\n    },\n    //TRAIGO TODAS LAS PREGUNTAS DE LA BBDD\n    async getExperts() {\n      try {\n        const response = await axios.get(\"http://localhost:3000/experts\");\n        this.experts = response.data.data;\n      } catch (error) {\n        this.showError = true;\n        this.errorMessage = error.response.data.message;\n      }\n    },\n  },\n  created() {\n    this.getExperts();\n  },\n};\n</script>\n\n<style scoped>\n.avatar {\n  width: 48px;\n  height: 48px;\n  border-radius: 24px;\n}\n</style>"]}]}