>\n</template>\n\n<script>\nexport default {\n  name: \"LanguageSelector\",\n  data() {\n    return {\n      option: \"\",\n      seeLanguages: false,\n      selectedLanguages: [],\n    };\n  },\n  props: {\n    languages: Array,\n  },\n  methods: {\n    //BUSCO EL NOMBRE DEL LENGUAJE PARA MOSTRARLO\n    getNameLanguage(index) {\n      this.seeLanguages = true;\n      this.selectedLanguages.push(this.languages[index].name_language);\n    },\n    //ESCUCHO LOS LENGUAJES SELECCIONADOS\n    //ENVÍO ID A LA VISTA\n    //ENVIO ID A LA FUNCION ANTERIOR\n    onChange(event) {\n      this.getNameLanguage(event.target.value);\n      let selectedUserLanguages = [];\n      selectedUserLanguages.push(event.target.value);\n      this.$emit(\"languageId\", selectedUserLanguages);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.avatar {\n  width: 48px;\n  height: 48px;\n  border-radius: 24px;\n}\n</style>"]}]}