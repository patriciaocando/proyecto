>\n      <p>{{ count }}</p>\n      <button v-show=\"seeButtonAnswer\" @click=\"showAnswers()\">VER RESPUESTAS</button>\n\n      <!--RESPUESTAS-->\n      <div v-show=\"seeAnswer\" class=\"answerInfo\" v-for=\"answer in answers\" :key=\"answer.id\">\n        <img class=\"avatar\" :src=\"question.answer[index].avatar\" />\n        <p>{{ question.answer[index].expert }}</p>\n        <p>{{ question.answer[index].answer }}</p>\n        <p>{{ question.answer[index].date }}</p>\n        <p>{{ question.answer[index].rating }} ({{ (question.answer[index].total_votes)-1}})</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"ShowQuestionsAnswer\",\n  data() {\n    return {\n      seeAnswer: false,\n      seeButtonAnswer: \"\",\n    };\n  },\n  props: {\n    questions: Array,\n    answers: Array,\n    count: String,\n  },\n  methods: {\n    showButtonAnswer() {\n      if (this.count === \"0\") {\n        this.seeButtonAnswer = true;\n      } else {\n        this.seeButtonAnswer = false;\n      }\n    },\n    showAnswers() {\n      if (this.seeAnswer === false) {\n        this.seeAnswer = true;\n      } else {\n        this.seeAnswer = false;\n      }\n    },\n    sendIndex(index) {\n      let questionData = this.questions[index].question;\n      console.log(\"hOLA QUE TAL!\", questionData);\n      this.$emit(\"questionData\", questionData);\n    },\n  },\n  created() {\n    this.showButtonAnswer();\n  },\n};\n</script>\n\n<style scoped>\n.questionInfo {\n  text-align: left;\n}\n.avatar {\n  width: 48px;\n  height: 48px;\n  border-radius: 24px;\n}\n\n.autorInfo {\n  display: flex;\n  flex-direction: row;\n}\n</style>\n"]}]}